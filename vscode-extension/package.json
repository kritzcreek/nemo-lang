{
  "name": "nemo-vscode",
  "description": "A VSCode extension for the nemo programming language",
  "author": "Christoph Hegemann",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kritzcreek/nemo-lang"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "activationEvents": [
    "onLanguage:plaintext"
  ],
  "main": "./out/extension",
  "contributes": {
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "dependencies": {
    "@types/vscode": "^1.75.1",
    "vscode-languageclient": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.18.34",
    "typescript": "^5.2.2"
  }
}
