use prim

import log : fn (I32) -> Unit from log

struct S { x : I32 }

fn main() {
  log(1 + 1);
  log(1 + 1 + 1);
  log(1 * 1 + 1);
  log(1 + 1 * 1);
  if 1 <= 2 && 1 < 1 || 1 >= 3 && 3 > 4 || 1 == 1 || 1 != 2 {
    log(1);
  } else {
    log(2);
  };

  log(1 + S { x = 10 }.x);

  if prim::f32_abs(2.5) > prim::f32_abs(prim::f32_neg(2.6)) {
    log(1);
  } else {
    log(2);
  };
}
