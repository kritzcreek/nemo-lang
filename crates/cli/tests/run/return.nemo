import log : fn (I32) -> Unit from log
import logf : fn (F32) -> Unit from log

fn main() -> I32 {
  log(early_return());
  logf(loop_return());
  log(lambda_return());
}

fn early_return() -> I32 {
  return 10;
  20;
}

fn loop_return() -> F32 {
  let i = 0.0;
  while true {
    if i > 10.0 {
        return i;
    } else {};
    set i = i + 1.0;
  };
  0.0;
}

fn lambda_return() -> I32 {
  let f = \() -> Bool {
    return true;
  };
  if f() {
    return 12;
  } else {
    return 20;
  };
}
