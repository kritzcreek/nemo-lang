import log : fn (i32) -> unit from log
import log_f32 : fn (f32) -> unit from log

fn main() {
  log(1 + 1);
  log(2 - 1);
  log(1 - 2);
  log(2 - 2);
  log(2 * 3);
  log(3 * 2);
  log(2 / 3);
  log(3 / 2);

  log_f32(1.1 + 1.1);
  log_f32(2.1 - 1.1);
  log_f32(1.1 - 2.1);
  log_f32(2.1 - 2.1);
  log_f32(2.1 * 3.1);
  log_f32(3.1 * 2.1);
  log_f32(2.1 / 3.1);
  log_f32(3.1 / 2.1);
}
