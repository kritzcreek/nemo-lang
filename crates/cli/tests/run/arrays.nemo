use prim

import log : fn (I32) -> Unit from log

fn main() {
  let empty: Array[I32] = [];
  let single = [1];
  let double = [1, 2];
  let double_trailing = [1, 2,];

  let nested : Array[Array[I32]] = [[], [42]];
  log(nested[1][0]);
  log([1][0]);

  let double = [1, 1];
  let triple = [2, 2, 2, 2];
  prim::array_copy(triple, 1, double, 0, 2);
  log(triple[0]);
  log(triple[1]);
  log(triple[2]);
  log(triple[3]);
}
